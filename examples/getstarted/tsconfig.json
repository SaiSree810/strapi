{
  "compilerOptions": {
    "lib": ["es6"],
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "noEmit": true,
    "declaration": false,
    "outDir": "./never",
    "types": ["@strapi/strapi"]
  },
  "include": ["global.d.ts", "**/*.js", "**/*.json", "**/*.mjs", "**/*.ts"]
}
