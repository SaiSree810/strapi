{
  "name": "@strapi/generators",
  "version": "5.0.0-beta.0",
  "description": "Interactive API generator.",
  "keywords": [
    "strapi",
    "generators"
  ],
  "homepage": "https://strapi.io",
  "bugs": {
    "url": "https://github.com/strapi/strapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/strapi/strapi.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Strapi Solutions SAS",
    "email": "hi@strapi.io",
    "url": "https://strapi.io"
  },
  "maintainers": [
    {
      "name": "Strapi Solutions SAS",
      "email": "hi@strapi.io",
      "url": "https://strapi.io"
    }
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "build:pack": "pack-up build",
    "build:copy": "copyfiles -u 1 -a 'src/templates/**/*' 'src/files/**/*' dist",
    "clean": "rimraf ./dist",
    "lint": "eslint .",
    "test:ts": "tsc --noEmit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "watch": "pack-up watch"
  },
  "dependencies": {
    "@sindresorhus/slugify": "1.1.0",
    "@strapi/typescript-utils": "workspace:*",
    "@strapi/utils": "workspace:*",
    "chalk": "4.1.2",
    "copyfiles": "2.4.1",
    "fs-extra": "11.2.0",
    "node-plop": "0.26.3",
    "plop": "2.7.6",
    "pluralize": "8.0.0"
  },
  "devDependencies": {
    "@strapi/pack-up": "workspace:*",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.2",
    "eslint-config-custom": "workspace:*",
    "npm-run-all": "4.1.5",
    "tsconfig": "workspace:*"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  }
